      "withProduced": false,
      "withWrote": true,
      "field": "{\"op\":\"eq\",\"field\":\"profession\",\"value\":\"writer\"}"
    },
    "query": "fragment EntityTinyFragment on Entity {\n  id: _id\n  title\n  __typename\n}\n\nfragment PersonTinyFragment on Person {\n  id: _id\n  name\n  __typename\n}\n\nquery people_writers($page: Int, $field: String, $withActed: Boolean!, $withProduced: Boolean!, $withDirected: Boolean!, $withWrote: Boolean!) {\n  people(\n    page: $page\n    field: $field\n    withActed: $withActed\n    withWrote: $withWrote\n    withDirected: $withDirected\n    withProduced: $withProduced\n  ) {\n    ...PersonTinyFragment\n    directed @include(if: $withDirected) {\n      id {\n        ...EntityTinyFragment\n        __typename\n      }\n      __typename\n    }\n    acted @include(if: $withActed) {\n      id {\n        ...EntityTinyFragment\n        __typename\n      }\n      __typename\n    }\n    produced @include(if: $withProduced) {\n      id {\n        ...EntityTinyFragment\n        __typename\n      }\n      __typename\n    }\n    wrote @include(if: $withWrote) {\n      id {\n        ...EntityTinyFragment\n        __typename\n      }\n      __typename\n    }\n    __typename\n  }\n}\n"
  }
