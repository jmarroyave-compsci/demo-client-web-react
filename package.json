{
  "name": "labs-web-client-react",
  "version": "2.5.2",
  "private": true,
  "scripts": {
    "dev": "cross-env BABEL_ENV=dev next dev",
    "build": "cross-env BABEL_ENV=dev next build && next export -o docs/app",
    "start": "cross-env BABEL_ENV=dev next build && next start",
    "test": "cross-env BABEL_ENV=test NODE_OPTIONS=--experimental-vm-modules jest",
    "test-cypress-local": "cypress open --config-file cypress.local.json",
    "test-cypress-local:headless": "cypress run --config-file cypress.local.json",
    "test-cypress": "cypress open --config-file cypress.json",
    "test-cypress:headless": "cypress run --config-file cypress.json",
    "e2e": "start-server-and-test start http://localhost:3000 test-cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 test-cypress:headless"
  },
