{
  "name": "learn-starter",
  "version": "1.2.2",
  "private": true,
  "scripts": {
    "dev": "cross-env BABEL_ENV=dev next dev",
    "build": "cross-env BABEL_ENV=dev next build && next export -o docs/app",
    "start": "cross-env BABEL_ENV=dev next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "cross-env BABEL_ENV=test NODE_OPTIONS=--experimental-vm-modules jest",
    "test-cypress": "cypress open",
    "test-cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 test-cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 test-cypress:headless",
    "sitemap": "next-sitemap --config next.sitemap.config.js --sourceDir ./.next"
  },
